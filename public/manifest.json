{
  "name": "chrome-extension-50mi-alert",
  "version": "1.0",
  "manifest_version": 2,
  "description": "stretch yourself every 50minutes!",
  "browser_action": {
    "default_popup": "index.html",
    "defalut_title": "Open the popup"
  },
  "permissions": [
    "alarms"
  ],
  "background":{
    "scripts":[
      "background.js"
    ],
    "persistent":false
  },
  "icons":{
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
